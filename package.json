{
  "name": "starwars-names-learning_test",
  "version": "1.1.0",
  "description": "Get random Star Wars names",
  "main": "src/index.js",
  "scripts": {
    "test": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js",
    "semantic-release": "semantic-release",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vsramanujan/starwars-names.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "author": "Ramanujan Veeravalli <rama.psbb@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vsramanujan/starwars-names/issues"
  },
  "homepage": "https://github.com/vsramanujan/starwars-names#readme",
  "dependencies": {
    "istanbul": "0.4.5",
    "unique-random-array": "1.0.1"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "codecov.io": "0.1.6",
    "commitizen": "3.0.5",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.4",
    "mocha": "5.2.0",
    "semantic-release": "^15.13.3"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}